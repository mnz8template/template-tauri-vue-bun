{
  "name": "template-tauri-vue-bun",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --config src-fe/vite.config.ts",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview --config src-fe/vite.config.ts",
    "test:unit": "vitest -c src-fe/vitest.config.ts",
    "test:e2e": "playwright test -c src-fe/playwright.config.ts",
    "build-only": "vite build --config src-fe/vite.config.ts",
    "type-check": "vue-tsc --build src-fe --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src-fe/src/",
    "tauri": "tauri"
  },
  "dependencies": {
    "pinia": "^2.2.4",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tauri-apps/cli": "^1.6.2",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.16.11",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.1",
    "eslint-plugin-playwright": "^1.7.0",
    "eslint-plugin-vue": "^9.29.0",
    "jsdom": "^24.1.3",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "typescript": "~5.4.5",
    "vite": "^5.4.8",
    "vite-plugin-vue-devtools": "^7.4.6",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.1.6"
  }
}