{
  "name": "template-tauri-vue-bun",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --config src-fe/vite.config.ts",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview --config src-fe/vite.config.ts",
    "test:unit": "vitest -c src-fe/vitest.config.ts",
    "test:e2e": "playwright test -c src-fe/playwright.config.ts",
    "build-only": "vite build --config src-fe/vite.config.ts",
    "type-check": "vue-tsc --build src-fe --force",
    "lint": "eslint ./src-fe --fix --ignore-path ../.gitignore",
    "format": "prettier --write src-fe/src/",
    "tauri": "tauri"
  },
  "dependencies": {
    "pinia": "^2.1.7",
    "vue": "^3.4.26",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.43.1",
    "@rushstack/eslint-patch": "^1.10.2",
    "@tauri-apps/cli": "^1.5.12",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.1.1",
    "eslint-plugin-playwright": "^1.6.0",
    "eslint-plugin-vue": "^9.25.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "typescript": "~5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-vue-devtools": "^7.1.3",
    "vitest": "^1.5.3",
    "vue-tsc": "^2.0.16"
  }
}